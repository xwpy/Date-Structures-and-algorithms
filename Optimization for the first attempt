import time

start_time = time.time()
print(start_time)
def container():
    for a in range(0,1001):
        for b in range(0,1001):
            c = 1000 - a - b
            if a**2 + b**2 == c**2:
                print("a, b, c : %d, %d, %d"%(a, b, c))
    print("finsihed")
container()
end_time = time.time()
use_time =(end_time) - (start_time)
print(use_time)
